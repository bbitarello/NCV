fd=/mnt/sequencedb/PopGen/barbara/simulations/slim_neutral/test
Rscript=/mnt/sequencedb/PopGen/barbara/simulations/scripts/SLiM_2pop_wOUT_trick_barbara.r
PARAMETERS=${fd}/parameters.tsv  #this file is changed for h=100
WORKING_DIRECTORY=${fd}/s2/   #alter this s1 every time I run more.##
SGE_SCRIPT=/mnt/sequencedb/PopGen/barbara/simulations/scripts/slim.sge
SGE_KEY_JOB=neutst2
LOGS_FOLDER=${fd}/logs/
/mnt/sequencedb/PopGen/barbara/simulations/scripts/run_SLiM.r ${Rscript} ${PARAMETERS} ${WORKING_DIRECTORY} ${SGE_SCRIPT} ${SGE_KEY_JOB} ${LOGS_FOLDER} barbara_domingues 200 > test2.log

